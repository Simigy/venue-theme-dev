{% comment %}
  Category Icons Section
  Displays category icons with links under the navbar
{% endcomment %}

{%- style -%}
/* === FORCE OVERRIDE ALL STYLES === */
.category-icons,
section.category-icons,
.shopify-section .category-icons {
  background-color: {{ section.settings.background_color }} !important;
  margin: 0 !important;
  position: relative !important;
  z-index: 1 !important;
}

/* Responsive padding-top for different screen sizes */
/* Default for 1440px and above */
.category-icons,
section.category-icons,
.shopify-section .category-icons {
  padding-top: 80px !important;
  padding-bottom: 12px !important;
  padding-left: 0px !important;
  padding-right: 0px !important;
}

/* 1200px, 1024px, 992px, 900px */
@media screen and (max-width: 1439px) and (min-width: 788px) {
  .category-icons,
  section.category-icons,
  .shopify-section .category-icons {
    padding-top: 150px !important;
  }
}

/* 787px */
@media screen and (max-width: 788px) and (min-width: 601px) {
  .category-icons,
  section.category-icons,
  .shopify-section .category-icons {
    padding-top: 185px !important;
  }
}

/* 600px - 2 rows layout (4 icons first row, 3 icons second row) */
@media screen and (max-width: 600px) and (min-width: 482px) {
  .category-icons,
  section.category-icons,
  .shopify-section .category-icons {
    padding-top: 75px !important;
  }

  .category-icons__grid {
    display: flex !important;
    flex-direction: row !important;
    flex-wrap: wrap !important;
    justify-content: center !important;
    gap: 16px !important;
  }

  .category-icons__item {
    flex: 0 0 calc(25% - 12px) !important;  /* 4 items per row */
    max-width: calc(25% - 12px) !important;
  }
  
  /* Make 5th, 6th, 7th items take up second row */
  .category-icons__item:nth-child(n+5) {
    flex: 0 0 calc(33.333% - 12px) !important;  /* 3 items per row */
    max-width: calc(33.333% - 12px) !important;
  }
}

/* 480px and below */
@media screen and (max-width: 481px) {
  .category-icons,
  section.category-icons,
  .shopify-section .category-icons {
    padding-top: 75px !important;
  }
}

.category-icons__container {
  max-width: 1200px !important;
  margin: 0 auto !important;
  padding: 0 15px !important;
}

.category-icons__grid {
  display: flex !important;
  justify-content: center !important;
  align-items: center !important;
  gap: 24px !important;
  flex-wrap: wrap !important;
}

.category-icons__item {
  display: flex !important;
  flex-direction: column !important;
  align-items: center !important;
  text-align: center !important;
}

.category-icons__label,
.category-icons__item p {
  display: block !important;
  text-align: center !important;
  margin: 8px auto 0 auto !important;
  width: 100% !important;
  font-size: 14px !important;
  line-height: 1.2 !important;
}

/* Responsive breakpoints with !important */
@media screen and (max-width: 1200px) and (min-width: 768px) {
  .category-icons__item:nth-child(n+6) { 
    display: none !important; 
  }
  
  /* Force logo to left on large screens where icons are hidden */
  .shopify-section-header .header {
    justify-content: flex-start !important;
  }
  
  .shopify-section-header .header__logo {
    margin-left: 0 !important;
    margin-right: auto !important;
    text-align: left !important;
  }
}

/* Large screens (1200px+) - ensure logo stays left */
@media screen and (min-width: 1200px) {
  .shopify-section-header .header {
    justify-content: flex-start !important;
  }
  
  .shopify-section-header .header__logo {
    margin-left: 0 !important;
    margin-right: auto !important;
    text-align: left !important;
  }
  
}

/* 601-767px: Horizontal row layout */
@media screen and (max-width: 767px) and (min-width: 601px) {
  .category-icons,
  section.category-icons,
  .shopify-section .category-icons {
    padding-top: 75px !important;
  }

  .category-icons__grid {
    display: flex !important;
    flex-direction: row !important;
    flex-wrap: wrap !important;
    justify-content: center !important;
    gap: 16px !important;
    overflow: visible !important;
  }

  .category-icons__item {
    flex: 0 0 auto !important;
    width: auto !important;
    text-align: center !important;
  }
}

/* 482-600px: 2 rows (4 icons + 3 icons) */
@media screen and (max-width: 600px) and (min-width: 482px) {
  .category-icons__grid {
    display: flex !important;
    flex-direction: row !important;
    flex-wrap: wrap !important;
    justify-content: center !important;
    gap: 16px !important;
    overflow: visible !important;
  }

  .category-icons__item {
    flex: 0 0 calc(25% - 12px) !important;
    max-width: calc(25% - 12px) !important;
    text-align: center !important;
  }
  
  .category-icons__item:nth-child(n+5) {
    flex: 0 0 calc(33.333% - 12px) !important;
    max-width: calc(33.333% - 12px) !important;
  }
}

/* Specific fix for Collaborations label */
.collaborations-label,
.collaborations-item p {
  display: flex !important;
  justify-content: center !important;
  align-items: center !important;
  text-align: center !important;
  margin: 8px auto 0 auto !important;
  width: 100% !important;
}

.collaborations-item {
  display: flex !important;
  flex-direction: column !important;
  align-items: center !important;
  text-align: center !important;
}

/* Reduce navbar height to prevent overlap - keep logo positioning */
.shopify-section-header .header,
.shopify-section-header .header-trigger {
  height: 60px !important;
  min-height: 60px !important;
}

.shopify-section-header .header__logo {
  height: 60px !important;
  min-height: 60px !important;
  /* Keep original positioning - don't change alignment */
}

.shopify-section-header .header__logo-img {
  max-height: 40px !important;
  /* Keep logo at left position for @1440px */
}

.shopify-section-header .header__logo-ratio-box {
  height: 40px !important;
}

/* Force logo to left position on large screens */
.shopify-section-header .header {
  justify-content: flex-start !important;
}

.shopify-section-header .header__logo {
  padding-top: 10px !important;
  padding-bottom: 10px !important;
  margin-left: 0 !important;
  margin-right: auto !important;
  text-align: left !important;
}

/* Ensure header is not centered */
.shopify-section-header .header.header--center {
  justify-content: flex-start !important;
}

.shopify-section-header .header.header--center .header__logo {
  text-align: left !important;
  margin-left: 0 !important;
  margin-right: auto !important;
}

/* === Base wrapper (applies to ALL icons) === */
.category-icons__image-wrapper,
.category-icons__image-wrapper--multi {
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: {{ section.settings.icon_border_radius }}%;
  background: {{ section.settings.icon_background }};
  width: {{ section.settings.icon_size }}px;
  height: {{ section.settings.icon_size }}px;
  margin-bottom: {{ section.settings.label_spacing }}px;
  border: {{ section.settings.icon_border_width }}px solid {{ section.settings.icon_border_color }};
  overflow: hidden;
  transition: all 0.3s ease;
}

/* === Image inside wrapper === */
.category-icons__image {
  width: 75%;          
  height: 75%;
  object-fit: contain; 
}

.category-icons__item {
  display: flex;
  flex-direction: column;
  align-items: center;
  text-align: center;
}

.category-icons__item p {
  display: block;
  text-align: center;
  margin: 8px auto 0 auto; 
  width: 100%;
  font-size: {{ section.settings.font_size }}px;
  font-weight: {{ section.settings.font_weight }};
  color: {{ section.settings.text_color }};
  line-height: 1.2;
}

.category-icons__label {
  text-align: center;
  display: block;
  width: 100%;
}

/* Multi-image tweaks */
.category-icons__image-wrapper--multi {
  display: flex;
  justify-content: center;
  align-items: center;
  gap: 2px; 
}

.category-icons__image-wrapper--multi .category-icons__image {
  flex: 1;            
  max-width: 35%;      
  height: auto;
  object-fit: contain;
}

/* === Per-icon desktop paddings === */
.category-icons__item:nth-child(1) .category-icons__image-wrapper { padding: {{ section.settings.icon1_padding_desktop }}; }
.category-icons__item:nth-child(2) .category-icons__image-wrapper { padding: {{ section.settings.icon2_padding_desktop }}; }
.category-icons__item:nth-child(3) .category-icons__image-wrapper { padding: {{ section.settings.icon3_padding_desktop }}; }
.category-icons__item:nth-child(4) .category-icons__image-wrapper { padding: {{ section.settings.icon4_padding_desktop }}; }
.category-icons__item:nth-child(5) .category-icons__image-wrapper { padding: {{ section.settings.icon5_padding_desktop }}; }
.category-icons__item:nth-child(6) .category-icons__image-wrapper { padding: {{ section.settings.icon6_padding_desktop }}; }
.category-icons__item:nth-child(7) .category-icons__image-wrapper { padding: {{ section.settings.icon7_padding_desktop }}; }

/* Hide some icons at mid breakpoints */
@media screen and (max-width: 1200px) and (min-width: 768px) {
  .category-icons__item:nth-child(n+6) { 
    display: none; 
  } /* show first 5 only */
}

/* This rule is removed - handled by specific breakpoints above */

/* Fix label alignment for "Collaborations" */
@media screen and (max-width: 600px) {
  .category-icons__item p.category-icons__label {
    display: block !important;
    text-align: center !important;
    margin: 0 auto !important;
    width: 100% !important;
  }
}

/* Mobile carousel - 3 icons sliding */
@media screen and (max-width: 480px) {
  .category-icons__grid {
    position: relative !important;
    height: 120px !important;
    overflow: hidden !important;
    flex-wrap: nowrap !important;
  }
  
  .category-icons__item {
    position: absolute !important;
    width: calc(33.333% - 8px) !important;
    transition: left 0.8s ease-in-out !important;
    top: 0 !important;
  }
  
  .category-icons__item.slot-0 { left: 0% !important; }
  .category-icons__item.slot-1 { left: 33.333% !important; }
  .category-icons__item.slot-2 { left: 66.666% !important; }
  .category-icons__item.slot-hidden { left: 100% !important; }
}
{%- endstyle -%}

